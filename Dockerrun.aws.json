{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "instagram",
        "image": "nithinsgowda/instagram-crawler",
        "essential": true,
        "memory": 256,
        "portMappings": [
            {
              "hostPort": 8000,
              "containerPort": 8000
            }
        ],
        "environment": [
          {
            "name": "Container",
            "value": "instagram"
          }
        ]
      },
      {
        "name": "data",
        "image": "sociolitic/data-crawlers",
        "essential": true,
        "memory": 256,
        "portMappings": [
            {
              "hostPort": 5000,
              "containerPort": 5000
            }
        ],
        "environment": [
          {
            "name": "Container",
            "value": "data"
          }
        ]
      },
      {
        "name": "master",
        "image": "nithinsgowda/sociolitic",
        "essential": true,
        "memory": 256,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 8080
          }
        ],
        "links": [
          "instagram",
          "data"
        ]
      }
    ]
  }